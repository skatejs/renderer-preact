{
  "devDependencies": {
    "@skatejs/ssr": "^0.12.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "conartist": "^0.9.2",
    "jest": "^20.0.4",
    "preact": "^8.0.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "scripts": {
    "build:es":
      "babel --no-babelrc src --out-dir es --presets=$(pwd)/config/babel.es",
    "build:esnext":
      "babel --no-babelrc src --out-dir esnext --presets=$(pwd)/config/babel.esnext",
    "build:node":
      "babel --no-babelrc src --out-dir node --presets=$(pwd)/config/babel.node",
    "test": "jest",
    "build:umd": "rollup -c && rollup -c --min",
    "build":
      "npm run build:es && npm run build:esnext && npm run build:node && npm run build:umd",
    "prepublish": "npm run build"
  },
  "jest": {
    "modulePathIgnorePatterns": ["./node_modules"],
    "testEnvironment": "@skatejs/ssr/jest"
  },
  "name": "@skatejs/renderer-preact",
  "version": "1.0.0",
  "description": "SkateJS renderer for Preact.",
  "main": "index.es5.js",
  "esnext": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skatejs/renderer-preact.git"
  },
  "author": "Trey Shugart <treshugart@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skatejs/renderer-preact/issues"
  },
  "homepage": "https://github.com/skatejs/renderer-preact#readme",
  "peerDependencies": {
    "preact": "^8.0.0"
  },
  "babel": {
    "presets": ["babel-preset-env"]
  },
  "files": ["index.js", "index.es5.js"]
}
